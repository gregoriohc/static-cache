{
    "name": "gregoriohc/static-cache",
    "type": "library",
    "description": "PHP simple static cache class",
    "keywords": [
        "gregoriohc",
        "static-cache"
    ],
    "homepage": "https://github.com/gregoriohc/static-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregorio HernÃ¡ndez Caso",
            "email": "gregoriohc@gmail.com",
            "homepage": "https://github.com/gregoriohc",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=4.0",
        "squizlabs/php_codesniffer": "^2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Gregoriohc\\StaticCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gregoriohc\\StaticCache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
